fuddbuzz

var any = false;
for (var i = 0; i < 99; i++) {
if (i%3 == 0) {
console.log("fuzz" + i)
any = true}
if (i%5 == 0) {
console.log("buzz" + i)
any = true}
if (any) {
console.log("fuzzbuzz")
any = false
}

}
